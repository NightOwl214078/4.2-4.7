                    typeWriter(".:mmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmh.+hhhhhhhhhhhyy+//+mmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmm:.");
                    typeWriter(".:mmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmm/:hhhhhhhhhyys+///ymmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmm:.");
                    typeWriter(".:mmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmsymmmmmmmmhsssssssmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmm:.");
                    typeWriter(".:mmmmmdhysoossydmmmmmdsssdmmmmmmsssdmmmmmmmmmmmdysshmmssssssssssssdmyssssssssssshmhsssssssssssshm:.");
                    typeWriter(".:mmmds+//+++++/+ohmmmh///hmmmmmd///ymmmmmmmmmmmd+//ymm+//++++++++/dmo///+++++++/yms++++++++/+//ym:`");
                    typeWriter(".:mmh+//+yhdddho///smmh///hmmmmmd///ymmmmmmmmmmmd+//ymm+//sddddddddmmo//oddddddddmmdddddddho//+ymm:.");
                    typeWriter(".:md+//odmmmmmmmy///ymh///hdddddd///ymmmmmmmmmmmd+//ymm+//smddmdddmmmo//odddddddmmmmmmmmmh+//odmmm:.");
                    typeWriter(".:mh///hmmmmmmmmd+//omh/////////////ymmmmmmmmmmmd+//ymm///+///////dmmo//////////ymmmmmmdo//+ymmmmm:.");
                    typeWriter(".:mh///smmmmmmmmd+//smh///ossssss///ymmmmmmmmmmmd+//smm///osssssssmmmo//+ssssssshmmmmmy+/+sdmmmmmm:.");
                    typeWriter("`:mmo//+ymmmmmmho//+dmh///hmmmmmd///ymmmmmmhdmmmh+//ymm+//smmmmmmmmmmo//ommmmmmmmmmmho//+hmmmmmmmm:.");
                    typeWriter(".:mmms+//+osyso/+/odmmh///hmmmmmd///ymmmmms/+oss+//+dmm+//ossssssssdmo//+ssssssssyds///+sssssssshm:.");
                    typeWriter(".:mmmmdso++///++shmmmmh++/hmmmmmd///hmmmmmyo+///++sdmmm++//++//++//hmo++///+/+/++oh+/++/+/+///+/ym:.");
                    typeWriter(".:dmmmmmmddhhddmmmmmmmmdddmmmmmmmddddmmmmmmmdddddmmmmmmdddddddddddddmdddddddddddddmddddddddddddddd:.");
                    typeWriter("....................................................................................................");
                }
            }
            else if(name.equals(h)){
                for(int makeLoop = 0; makeLoop < 1; makeLoop++){
                    typeWriter("                             `------.`        `.--:::::--`                              ```                 ");    
                    typeWriter("`/::::::::::::-....-:::::::::::-........-:/:::----:::::/:/::-.``   `.::/:::::-.`  ``.----.`     ");
                    typeWriter("`+...........................................................-::::::-........--:::::-....-:/.   ");
                    typeWriter(" /:..............--........................................................................-+   ");
                    typeWriter(" `+-..........-/+++-......................................................................./:   ");
                    typeWriter("  `+.........:+++++-....................................................................../:    ");
                    typeWriter("   ::.......-+++++/....................................................................../-     ");
                    typeWriter("   +-......./++++:...............................................................::......:/     ");
                    typeWriter("  -/........-//:-..............................................................-/++......./-    ");
                    typeWriter("  /:........................-:://////:-....................-----...............+++:........+    ");
                    typeWriter("  .+....................-//+/-..```..-:++/-..........-//+//:::::////:-.........:--.........+    ");
                    typeWriter("   ./-.....://........-++-`             `-/+/....../+/-.`         `.:++:................../-    ");
                    typeWriter("    .+..../++/......./o.                   `/o-..:o:`                 ./o:..............:/.     ");
                    typeWriter("    .+....++/-......++`            `-:::-`   -s:/o.    ``..`            .o+............::`      ");
                    typeWriter("    /-....--.......:s            `+yyyyyyy+.  .ys`   -oyyyyys:`          `o/.........../.       ");
                    typeWriter("   -/..............s:            sdssdmdyshy`  y-   :dysyhhsyds`          `y-..........-+`      ");
                    typeWriter("   /-..............s-           `yhsyNNNysyd` `s`   shssNMNhssm.           o:.........../.      ");
                    typeWriter("   .+........--::::o+`           -yhyyyysyh/ `.s-..`-dysyhhyshy`           o/...........+.      ");
                    typeWriter("    -/-....-//:--:+/:/.           `:ossss+. `/:---:/.-oyyyyyy+`         `.-s+///:-....:/-       ");
                    typeWriter("     `+-...+//:.:/--.-+`              `     .+.....-+  `....`           +/-/+---:/+-..o`        ");
                    typeWriter("      /-...+:...-oy/..o+.`                 `:o.....::                  `o...-+/.-//o..+`        ");
                    typeWriter("     `+.....:-..--/s-..:++-.`           `.:+//:...:o+-`              `./o:.-os+-..:o..:/        ");
                    typeWriter("     +-............/s:....:/+/:-.....-:/+/:.....-//.-/+/:.````````.-/++-..-+s-...:+-...o`       ");
                    typeWriter("    `o..............-oo-......-:://:::-.........:-.....-:////++////:-...-/s/.....-.....o`       ");
                    typeWriter("    `+-...............:oo:-..........................................-:os/-...........-+        ");
                    typeWriter("     `/:................-+o+:-...................................--/oo+-..............+.        ");
                    typeWriter("       -/-.........::......-/++o/:-........................--:/oo++:................./-         ");
                    typeWriter("        `o.......:+++...........-/+++os+++////////+++++++y+++/-.........-/++:.......+.          ");
                    typeWriter("        ::......-+++-................-y` ````/o--/s`     s:............/++++/.......o`          ");
                    typeWriter("        o........::..................:y.`    /+../s     `+/.........../+++++:......./-          ");
                    typeWriter("        +.............................://+++++-...+++++++/...........-+++++:........+.          ");
                    typeWriter("        .+............:+++:..........................................-/+/:.........:/           ");
                    typeWriter("         -s:........-+++++:..........--....---------....--.......................:s-            ");
                    typeWriter("         o-:/......-+++++/....................................................../-o:            ");
                    typeWriter("        `y  -/.....:++++/-.............................................://......o``y`           ");
                    typeWriter("        :+  +-.....-///-............................................../++/....../- +:           ");
                    typeWriter("        o: `o...........---...........---............................-++:........+`.s           ");
                    typeWriter("        o-  :/::://++///:::+o//://++++/::/+++/-...-:+///////-.....-::/+///+/-...:/ `y           ");
                    typeWriter("        :o.```+o-.```       `...../o/.`    `.odhyhds.`````.+y+///:::.``````-:/y:.``-s           ");
                    typeWriter("         -:+s/oo                   `./+/:--++hhyhhy/+:-:///.`                 y+/y/:.           ");
                    typeWriter("           `y.-h                       `...``:dhhd: `--.`                     h--y`             ");
                    typeWriter("            y-.yo+++++++++++++//////////////+hhyyyh+//////////////////////////y.:s              ");
                    typeWriter("            ++.+y+++yyyyhhhho+++ohhhhhhhhho+hhyyyyhdosddddddddo++oyyyyyhyyy++yo.o/              ");
                    typeWriter("            .y.-h/++hmmmmmmmo/+++ddddddddd+odyyyyyydysdddddddd+//ommmmmmmmd+/h:.y.              ");
                    typeWriter("             s:.so+/+++++++++/+/++++++++++++shhyyhhs++++++++++//+++++++++++++h-.y`              ");
                    typeWriter("             -y.:h++/////////////////////////+shhs+/////////////////////////ss./o               ");
                    typeWriter("              o/.+soooo+++++++++++++////+////+/++/+++++++++++++oooo+oooo+oooh:.y.               ");
                    typeWriter("              :o../++y::::///ohsssssssssyyooooooooo+++ohyssssssoooho:::::ys/:..y`               ");
                    typeWriter("             `o/..-s/s-      :yo++++++ooss````         /oosooooooos:    /+/+...s:               ");
                    typeWriter("             -s.-.-h/s-      `.-//oy++y/.`              `:y::y/-.`      +++s...-y`              ");
                    typeWriter("             ++.o-.:yy-           `y-.++                 o/.-s`         -ys+..+.++              ");
                    typeWriter("             /+./s--y+o/.          ++.-y.               .s..o/         .+oh-.+s.:o              ");
                    typeWriter("             .s-./oy++s+s          .y--o+               os::y.        -s+sosso-.o:              ");
                    typeWriter("              .o/--/+s:h.           hdhdh`             `hhhdy`        -sy:+o:.-++`              ");
                    typeWriter("               `-/+++y+/            oyosy-             :sooso          :ooy-:++-                ");
                    typeWriter("                  ```               +o::o+             +s++s/           .-::-`                  ");
                    typeWriter("                                    +s+o+s             y:::+o                                   ");
                    typeWriter("                          ````     `s.  `s            `y.  `y:`  ``.--.``                       ");
                    typeWriter("                       `/ydmddho--ymm:``:m/          `yNo-:sNMmoodNNMMMNy-                      ");
                    typeWriter("                      `hMMMMMMMMMNMMMMNmNMN-         .dMMMMMMMMMMMMMMMMMMN.                     ");
                    typeWriter("                      -MMMMMMMMMMMMMMMMMMm:           .dMMMMMMMMMMMMMMMMMN-                     ");
