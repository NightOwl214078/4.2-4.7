                    typeWriter("--sooooooooooooooossssy:`   /MMMMMMMMh       /     hMMMMMMMM:     -o................................");
                    typeWriter(":soooooooooooooooooooossso/:.omNMMNd+` `.-:+ss::-...ymNNMNd+::/+oy+.................................");
                    typeWriter("yoooooooooooooooooooooosssssssoossoooossssssyooooossssssssooooos/-..................................");
                    typeWriter("sooooooooooooooooooooooooooosssssssssssyysooooooooooooooooooss+.....................................");
                    typeWriter("yooooooooooooooooooooooooooooooooossssssooooooosssyyssssssyo........................................");
                    typeWriter("yoooooooooooooooooooooooooooooossssoooooooooooooooosoooooooss:......................................");
                    typeWriter("yoooooooooooooooooooooooooooooooooooooooooooooooooooooooooooos+.....................................");
                    typeWriter("yooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooos/....................................");
                    typeWriter("soooooooooooooooooooosssssssooooooooooooooooooooooooooooooooooos....................................");
                    typeWriter("yooooooooooooooooosysssooosssssssssssssooooooooooooooooooossssysy:..................................");
                    typeWriter("soooooooooooooooosyooosyyssysssoooooossssssssssssssssssssssooooso-..................................");
                    typeWriter("sooooooooooooooooysoooooooooossssssssssyssssssssssssssssssyssy+-....................................");
                    typeWriter("sooooooooooooooyoosyyssssssssyysssooooooossssssssssssssssoooos+.....................................");
                    typeWriter("osooooooooooooosyssooooooooooooossssssyssssyyyyssssssssssssso/......................................");
                    typeWriter("/syssooooooooooooossooooooooooooooooooooooooooooosssssyo/-..........................................");
                    typeWriter("hhhyyyssssoooooooooooooooooooooooooooooooooooooooosso/-.............................................");
                    typeWriter("hyyyhhhyyysssssssssssoooooooooooooooooooooooosso+/:.................................................");
                    typeWriter("hyyyyyyyhhhyyysssoosssssssssssssssssssssssshhh/.....................................................");
                }
            }
            else
            {
                typeWriter("Invalid Input! Try again!");
            }
            typeWriter("Thank you for playing! hope you had a laugh.");
            typeWriter("The compiler will now bark at you:");
            try
            {
                Thread.sleep(1800);
            } 
            catch(InterruptedException ex)
            {
                Thread.currentThread().interrupt();
            }               
            for(;;){
                typeWriter("───█───▄▀█▀▀█▀▄▄───▐█──────▄▀█▀▀█▀▄▄");
                typeWriter("──█───▀─▐▌BA ▐▌─▀▀──▐█─────▀─▐▌ RK ▐▌─█▀");
                typeWriter("─▐▌──────▀▄▄▀──────▐█▄▄──────▀▄▄▀──▐▌");
                typeWriter("─█────────────────────▀█────────────█");
                typeWriter("▐█─────────────────────█▌───────────█");
                typeWriter("▐█─────────────────────█▌───────────█");
                typeWriter("─█───────────────█▄───▄█────────────█");
                typeWriter("─▐▌───────────────▀███▀────────────▐▌");
                typeWriter("──█──────────▀▄───────────▄▀───────█");
                typeWriter("───█───────────▀▄▄▄▄▄▄▄▄▄▀────────█\n");
            }
        }
    }
}

