            else if(name.equals(j)){
                for(int makeLoop = 0; makeLoop < 1; makeLoop++){
                    typeWriter("-................:+ssoooooooooosso+-...:/+osssooossso:.............................................:");
                    typeWriter("-............../ssooooooooooooooooosssssoooooooooooooss:...........................................:");
                    typeWriter("-............/sooooooooooosssssoooooosyoooooooooooooooos:..........................................-");
                    typeWriter("-...........osooooooosssssssossssssssosyooooooooooooooooy-........................................./");
